<div class="gallery js-flickity">
  <% tracks.each do |track|%>
    <% unless track.nil? %>
      <div class="gallery-cell">
        <%= image_tag track.cover_image, class: 'gallery-image' %>
        <label class="track-title"><%= track.title%></label>
        <% if track.user.display_name.nil? || track.user.display_name == ""%>
          <%= "#{track.user.first_name} #{track.user.last_name}" %>
        <% else %>
          <%= track.user.display_name %>
        <% end %>
      </div>
    <% end %>
  <% end %>
</div>